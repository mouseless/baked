{
  "plugins": [
    {
      "name": "auth",
      "anonymousApiRoutes": [
        {
          "method": "POST",
          "path": "form-sample/parents"
        },
        {
          "method": "DELETE",
          "path": "form-sample/parents"
        },
        {
          "method": "GET",
          "path": "form-sample/parents"
        },
        {
          "method": "GET",
          "path": "route-parameters-samples/{id}"
        },
        {
          "method": "GET",
          "path": "route-parameters-samples/{id}/items"
        },
        {
          "method": "GET",
          "path": "cache-samples/scoped"
        },
        {
          "method": "GET",
          "path": "cache-samples/application"
        },
        {
          "method": "POST",
          "path": "authorization-samples/anonymous"
        },
        {
          "method": "POST",
          "path": "authentication-samples/login"
        }
      ],
      "anonymousPageRoutes": [
        "^(?!.*auth).*$",
        "login"
      ],
      "loginPageRoute": "login",
      "refreshApiRoute": "authentication-samples/refresh",
      "basePath": "./runtime/plugins/",
      "resolver": "MetaUrl"
    },
    {
      "expirationInMinutes": 60,
      "basePath": "./runtime/plugins/",
      "name": "cacheApplication",
      "resolver": "MetaUrl"
    },
    {
      "expirationInMinutes": 60,
      "basePath": "./runtime/plugins/",
      "name": "cacheUser",
      "resolver": "MetaUrl"
    },
    {
      "handlers": [
        {
          "statusCode": 401,
          "behavior": "Redirect",
          "behaviorArgument": {
            "type": "Computed",
            "composable": "useLoginRedirect"
          },
          "order": 0
        },
        {
          "statusCode": 400,
          "behavior": "Alert",
          "order": 0
        },
        {
          "behavior": "Page",
          "order": 0
        }
      ],
      "defaultAlertTitle": "Unexpected Error",
      "defaultAlertMessage": "Please contact system administrator",
      "basePath": "./runtime/plugins/",
      "name": "errorHandling",
      "resolver": "MetaUrl"
    },
    {
      "supportedLanguages": [
        {
          "code": "en",
          "name": "English"
        },
        {
          "code": "tr",
          "name": "Turkish"
        }
      ],
      "basePath": "./runtime/plugins/",
      "name": "localization",
      "resolver": "MetaUrl"
    },
    {
      "basePath": "./app/plugins/",
      "name": "sample",
      "resolver": "RootDir"
    }
  ],
  "error": {
    "type": "ErrorPage",
    "schema": {
      "errorInfos": {
        "403": {
          "title": "Access Denied",
          "message": "You do not have the permission to view the address or data specified.",
          "showSafeLinks": true
        },
        "404": {
          "title": "Page Not Found",
          "message": "The page you want to view is either deleted or outdated.",
          "showSafeLinks": true
        },
        "500": {
          "title": "Unexpected Error",
          "message": "Please contact system administrator."
        },
        "999": {
          "title": "Application Error",
          "message": "Please contact system administrator."
        },
        "503": {
          "title": "Service Unavailable",
          "message": "The service is currently unavailable. Please try again later.",
          "customMessage": true
        }
      },
      "safeLinks": [
        {
          "type": "CardLink",
          "schema": {
            "route": "/",
            "icon": "pi pi-home",
            "title": "Home"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/cache-samples",
            "icon": "pi pi-database",
            "title": "Cache",
            "description": "Showcases the cache behavior"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/form-sample",
            "icon": "pi pi-file-edit",
            "title": "Form",
            "description": "Showcases action, form components and events"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/report-page-sample",
            "icon": "pi pi-file",
            "title": "Report",
            "description": "Showcases a report layout with tabs and data panels"
          }
        },
        {
          "type": "CardLink",
          "schema": {
            "route": "/specs",
            "icon": "pi pi-list-check",
            "title": "Specs",
            "description": "All UI Specs are listed here"
          }
        }
      ]
    },
    "data": {
      "type": "Computed",
      "composable": "useNuxtError"
    }
  },
  "i18n": {
    "defaultLanguage": {
      "code": "en",
      "name": "English"
    },
    "supportedLanguages": [
      {
        "code": "en",
        "name": "English"
      },
      {
        "code": "tr",
        "name": "Turkish"
      }
    ]
  }
}